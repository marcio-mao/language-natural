'.source.natural':
  editor:
    autoIndentOnPaste: false
    tabLength: 2
    preferredLineLength: 72
    commentStart: "*> "
    increaseIndentPattern: "^\\s*(else|else\\sif|perform|if|add|calldelete|divide|evaluate|read|rewrite|start|string|subtract|unstring|write).*$"
    decreaseIndentPattern: "^\\s*(else|end-if|else\\sif|end-perform|end-add|end-call|end-delete|end-divide|end-evaluate|end-read|end-rewrite|end-start|end-string|end-subtract|end-unstring|end-write).*$"
